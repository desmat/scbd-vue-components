{
  "name": "my-vue-library",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": ["dist", "dist-types", "README.md", "LICENSE"],
  "main": "./dist/my-vue-library.umd.js",
  "module": "./dist/my-vue-library.es.js",
  "types": "./dist-types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist/my-vue-library.es.js",
      "require": "./dist/my-vue-library.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "sideEffects": ["*.css"],
  "scripts": {
    "dev": "vite",
    "build:types": "vue-tsc -p tsconfig.json",
    "build": "vite build && vue-tsc -p tsconfig.json",
    "prepare": "vite build && vue-tsc -p tsconfig.json",
    "prepack": "vite build && vue-tsc -p tsconfig.json",
    "clean": "rm -rf ./dist && rm -rf ./dist-types"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "sass-embedded": "^1.93.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {}
}
